{
  "name": "rei-dashboard",
  "version": "0.0.1",
  "description": "REI Dashboard",
  "repository": {
    "type": "git",
    "url": "git://github.com/vigetlabs/rei-dashboard.git"
  },
  "browser": {
    "c3": "./src/javascript/vendor/c3.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "c3": {
      "exports": "c3",
      "depends": [
        "d3"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "~1.3.6",
    "browserify": "~5.10.1",
    "browserify-shim": "^3.8.0",
    "gulp": "^3.8.7",
    "gulp-autoprefixer": "^1.0.1",
    "gulp-changed": "^0.4.1",
    "gulp-imagemin": "^1.0.1",
    "gulp-notify": "^1.4.2",
    "gulp-sass": "^0.7.3",
    "gulp-swig": "^0.7.2",
    "gulp-util": "^3.0.0",
    "node-sass": "^0.9.3",
    "pretty-hrtime": "~0.2.1",
    "require-dir": "^0.1.0",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "~1.0.2"
  },
  "dependencies": {
    "d3": "^3.4.11",
    "jquery": "~2.1.0"
  }
}
