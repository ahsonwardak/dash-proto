{
  "name": "rei-dashboard",
  "version": "0.0.1",
  "description": "REI Dashboard",
  "repository": {
    "type": "git",
    "url": "git://github.com/vigetlabs/rei-dashboard.git"
  },
  "browser": {
    "c3": "./src/javascript/vendor/c3.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "c3": {
      "exports": "c3",
      "depends": [
        "d3"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "2.11.2",
    "browserify": "13.0.0",
    "browserify-shim": "3.8.12",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "3.1.0",
    "gulp-changed": "1.3.0",
    "gulp-imagemin": "2.4.0",
    "gulp-notify": "2.2.0",
    "gulp-sass": "2.2.0",
    "gulp-swig": "0.8.0",
    "gulp-util": "3.0.7",
    "node-sass": "3.4.2",
    "pretty-hrtime": "1.0.2",
    "require-dir": "0.3.0",
    "vinyl-source-stream": "1.1.0",
    "watchify": "3.7.0"
  },
  "dependencies": {
    "d3": "3.5.16",
    "jquery": "2.2.3"
  }
}
